{
  "gitbook": "3.0.0-pre.15",
  "plugins": [
    "include-codeblock",
    "advanced-emoji",
    "prism",
    "-highlight",
    "copy-code-button",
    "edit-link",
    "expandable-chapters"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/ngrx/ngrx.github.io/tree/master",
      "label": "Edit This Page"
    },
    "expandable-chapters":{},
    "fontSettings": {
            "theme": "night",
            "family": "sans",
            "size": 2
        }
  }
}